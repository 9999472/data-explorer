{
  "name": "data-explorer",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Automatic data visualization with the nteract data-explorer",
  "keywords": [
    "hydrogen",
    "jupyter",
    "nteract",
    "pandas",
    "data"
  ],
  "consumedServices": {
    "hydrogen.provider": {
      "versions": {
        "^1.2.0": "consumeHydrogen"
      }
    }
  },
  "repository": "https://github.com/BenRussert/data-explorer.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "prettier": "prettier --write 'lib/**/*.js' 'spec/**/*.js'",
    "flow": "flow"
  },
  "dependencies": {
    "@nteract/transform-dataresource": "^4.3.2",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styled-jsx": "^3.0.2"
  },
  "package-deps": [
    "hydrogen"
  ],
  "devDependencies": {
    "atom-jasmine3-test-runner": "^4.0.0",
    "flow-bin": "^0.79.1",
    "prettier": "^1.13.5"
  },
  "atomTestRunner": "atom-jasmine3-test-runner"
}
